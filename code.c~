/***********************************All the external include files please write here********************************/
#include<stdio.h>
#include<stdlib.h>
#include<math.h>
#include<stdbool.h>
#include "prime.h"

/**************************************************Includes ends here*************************************************/

/*******************************************All #defines please include here ******************************************/
#define nl() printf("\n");
#define os() printf(" ");
#define sf(n) scanf("%d",&n);
#define pf(n) printf("%d",n);

/*********************************************#defines ends here******************************************************/





int main(){
	int n;
	do{
		sf(n);
		if(n==0)break;
		int *array=(int *)malloc(sizeof(int)*n);
		int i;
		pf(n);
		for(i=0;i<n;++i){
			sf(array[i]);
		}
		
		// now the processing 
		int count=(count_prime(array));
		printf("num of primes=");
		pf(count);
		nl();
	}while(n!=0);
	// will also send some packets but later
	
	return 0;
}
