#include<stdlib.h>
#include<stdio.h>
#include<math.h>
#include<limits.h>
#include<stdbool.h>

bool is_prime(int item){
	
	double y=(double)item;
	double x=sqrt(y);
	int i;
	int end=(int)x;
	for(i=2;i<(int)x;++i){
		if(item%i==0){
			return 0;
		}
		
	}
	return 1;

}
int count_prime(int *array,int s){
	int i=0,j=0;	
	for(i=0;i<s;++i){
		printf("item=%d\n",array[i]);
		if(array[i]>=1 && is_prime(array[i])){
			j++;
		}
	}
	return j;
}
